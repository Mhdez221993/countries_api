datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  name      String
  email     String   @unique
  password  String?
}

model Country {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  name      String
}
